<?xml version="1.0" encoding="UTF-8"?>
<codesense>
	<provider>
		<selector>text.latex, latex.part, latex.section, latex.subsection, latex.subsubsection, latex.paragraph, latex.subparagraph, latex.subsubparagraph</selector>
		
		<completions>net.texasexpat.latex.simple</completions>
		<completions>net.texasexpat.latex.functions</completions>
		<completions>net.texasexpat.latex.functions.binary</completions>
		<completions>net.texasexpat.latex.functions.binaryalt</completions>
		<complete-match>[\?\$\\a-zA-Z0-9-_\>\-]+</complete-match>
	</provider>

	<provider>
		<selector>latex.mathmode</selector>
		
		<completions>net.texasexpat.latex.functions.math.simple</completions>
		<complete-match>[\?\$\\a-zA-Z0-9-_\>\-]+</complete-match>
	</provider>
</codesense>