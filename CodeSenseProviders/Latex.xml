<?xml version="1.0" encoding="UTF-8"?>
<codesense>
	<provider>
		<selector>text.latex, latex.part, latex.chapter, latex.section, latex.subsection, latex.subsubsection, latex.paragraph, latex.subparagraph, latex.subsubparagraph</selector>
		
		<completions>net.texasexpat.latex.simple</completions>
		<completions>net.texasexpat.latex.functions</completions>
		<completions>net.texasexpat.latex.zone.equation</completions>
		<completions>net.texasexpat.latex.zone.equationarray</completions>
		
		<complete-match capture="1">(\\[a-zA-Z0-9]*)</complete-match>
	</provider>

	<provider>
		<selector>latex.mathmode.short, latex.mathmode.function</selector>
		
		<completions>net.texasexpat.latex.mathmode.simple</completions>
		<completions>net.texasexpat.latex.mathmode.functions</completions>
		<completions>net.texasexpat.latex.mathmode.functions.binary</completions>
		<completions>net.texasexpat.latex.mathmode.functions.binaryalt</completions>
		
		<complete-match capture="1">(\\[a-zA-Z0-9]*)</complete-match>
	</provider>
</codesense>